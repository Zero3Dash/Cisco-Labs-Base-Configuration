!------------------------------------------------------
! R1
!------------------------------------------------------
!
configure terminal
interface Ethernet0/0
 ip address 10.1.1.1 255.255.255.0
 no shutdown
exit
interface Serial1/0
 ip address 10.1.13.1 255.255.255.0
 no shutdown
exit
router ospf 1
 router-id 1.1.1.1
 network 10.1.13.1 0.0.0.0 area 0
 network 10.1.1.1 0.0.0.0 area 0
exit
end
write memory
!
